let vladniNarizeni = false

let skola = {
    typ: "Gymnazium",
    ulice: "Koukalova 12",
    mesto: "Ostrava",
    kapacita: 93,
    open: function(){
        console.log(`Škola ${this.typ} ${this.mesto} je Otevřená`)
    },
    close: function(){
        console.log(`Škola ${this.typ} ${this.mesto} je Zavřená`)
    }
}

if (vladniNarizeni){
    skola.close()
} else{
    skola.open()
}
